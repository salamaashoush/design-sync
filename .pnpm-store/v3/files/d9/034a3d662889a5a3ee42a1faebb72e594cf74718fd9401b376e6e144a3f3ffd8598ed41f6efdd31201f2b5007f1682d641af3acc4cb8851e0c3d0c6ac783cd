import { OverrideComponentProps } from "@kobalte/utils";
import { AsChildProp } from "../polymorphic";
export interface MenuItemIndicatorOptions extends AsChildProp {
    /**
     * Used to force mounting when more control is needed.
     * Useful when controlling animation with SolidJS animation libraries.
     */
    forceMount?: boolean;
}
export interface MenuItemIndicatorProps extends OverrideComponentProps<"div", MenuItemIndicatorOptions> {
}
/**
 * The visual indicator rendered when the parent menu `CheckboxItem` or `RadioItem` is checked.
 * You can style this element directly, or you can use it as a wrapper to put an icon into, or both.
 */
export declare function MenuItemIndicator(props: MenuItemIndicatorProps): import("solid-js").JSX.Element;
