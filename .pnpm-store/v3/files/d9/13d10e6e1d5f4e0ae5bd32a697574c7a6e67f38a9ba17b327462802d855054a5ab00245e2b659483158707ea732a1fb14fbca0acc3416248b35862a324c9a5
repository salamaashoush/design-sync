declare function clearViewport(): void;
declare function error(message: string, option?: {
    clearPreviousLine: boolean;
}): void;
declare function info(message: string, option?: {
    clearPreviousLine: boolean;
}): void;
declare function success(message: string, option?: {
    clearPreviousLine: boolean;
}): void;
export declare const log: {
    clearViewport: typeof clearViewport;
    error: typeof error;
    info: typeof info;
    success: typeof success;
};
export {};
