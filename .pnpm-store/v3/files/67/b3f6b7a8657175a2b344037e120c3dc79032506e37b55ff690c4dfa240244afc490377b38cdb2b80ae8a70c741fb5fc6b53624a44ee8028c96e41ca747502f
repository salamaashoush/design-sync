import { OverrideComponentProps } from "@kobalte/utils";
import { JSX } from "solid-js";
import { AsChildProp } from "../polymorphic";
export interface ContextMenuTriggerOptions extends AsChildProp {
    /** Whether the context menu trigger is disabled. */
    disabled?: boolean;
    /** The HTML styles attribute (object form only). */
    style?: JSX.CSSProperties;
}
export interface ContextMenuTriggerProps extends OverrideComponentProps<"div", ContextMenuTriggerOptions> {
}
export declare function ContextMenuTrigger(props: ContextMenuTriggerProps): JSX.Element;
