import { OverrideComponentProps } from "@kobalte/utils";
import { JSX } from "solid-js";
import { AsChildProp } from "../polymorphic";
import { DateSegment } from "./types";
export interface DatePickerSegmentOptions extends AsChildProp {
    segment: DateSegment;
}
export interface DatePickerSegmentProps extends OverrideComponentProps<"div", DatePickerSegmentOptions> {
}
export declare function DatePickerSegment(props: DatePickerSegmentProps): JSX.Element;
