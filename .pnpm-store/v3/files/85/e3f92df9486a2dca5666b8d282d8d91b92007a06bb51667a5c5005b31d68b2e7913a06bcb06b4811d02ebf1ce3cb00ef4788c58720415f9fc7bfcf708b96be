import { OverrideComponentProps } from "@kobalte/utils";
import * as Collapsible from "../collapsible";
export interface AccordionContentOptions extends Collapsible.CollapsibleContentOptions {
}
export interface AccordionContentProps extends OverrideComponentProps<"div", AccordionContentOptions> {
}
/**
 * Contains the content to be rendered when the `Accordion.Item` is expanded.
 */
export declare function AccordionContent(props: AccordionContentProps): import("solid-js").JSX.Element;
