import { OverrideComponentProps } from "@kobalte/utils";
import { AsChildProp } from "../polymorphic";
export interface ListboxItemIndicatorOptions extends AsChildProp {
    /**
     * Used to force mounting when more control is needed.
     * Useful when controlling animation with SolidJS animation libraries.
     */
    forceMount?: boolean;
}
export interface ListboxItemIndicatorProps extends OverrideComponentProps<"div", ListboxItemIndicatorOptions> {
}
/**
 * The visual indicator rendered when the item is selected.
 * You can style this element directly, or you can use it as a wrapper to put an icon into, or both.
 */
export declare function ListboxItemIndicator(props: ListboxItemIndicatorProps): import("solid-js").JSX.Element;
