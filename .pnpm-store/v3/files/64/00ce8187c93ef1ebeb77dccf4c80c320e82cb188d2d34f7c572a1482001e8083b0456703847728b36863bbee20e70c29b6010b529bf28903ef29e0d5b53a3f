import { Accessor } from "solid-js";
export interface MenuRootContextValue {
    isModal: Accessor<boolean>;
    preventScroll: Accessor<boolean>;
    forceMount: Accessor<boolean>;
    generateId: (part: string) => string;
}
export declare const MenuRootContext: import("solid-js").Context<MenuRootContextValue>;
export declare function useMenuRootContext(): MenuRootContextValue;
