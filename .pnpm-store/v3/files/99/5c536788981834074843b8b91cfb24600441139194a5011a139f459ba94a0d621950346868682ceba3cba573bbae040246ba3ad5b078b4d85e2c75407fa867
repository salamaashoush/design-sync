import { ToastConfig } from "./types";
declare function add(toast: ToastConfig): void;
declare function get(id: number): ToastConfig;
declare function update(id: number, toast: ToastConfig): void;
declare function dismiss(id: number): void;
declare function remove(id: number): void;
declare function clear(): void;
export declare const toastStore: {
    toasts: () => ToastConfig[];
    add: typeof add;
    get: typeof get;
    update: typeof update;
    dismiss: typeof dismiss;
    remove: typeof remove;
    clear: typeof clear;
};
export {};
