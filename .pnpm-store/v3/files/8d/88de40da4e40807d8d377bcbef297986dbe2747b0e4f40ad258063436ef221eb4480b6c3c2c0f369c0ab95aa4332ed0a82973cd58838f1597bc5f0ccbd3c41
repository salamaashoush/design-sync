import { Accessor } from "solid-js";
export interface MenuRadioGroupContextValue {
    isDisabled: Accessor<boolean | undefined>;
    isSelectedValue: (value: string) => boolean;
    setSelectedValue: (value: string) => void;
}
export declare const MenuRadioGroupContext: import("solid-js").Context<MenuRadioGroupContextValue>;
export declare function useMenuRadioGroupContext(): MenuRadioGroupContextValue;
