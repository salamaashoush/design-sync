import type { FieldValues, FormStore, Maybe, ResponseData } from '../types';
/**
 * Updates the invalid state of the form.
 *
 * @param form The store of the form.
 * @param dirty Whether there is an error.
 */
export declare function updateFormInvalid<TFieldValues extends FieldValues, TResponseData extends ResponseData>(form: FormStore<TFieldValues, TResponseData>, invalid?: Maybe<boolean>): void;
