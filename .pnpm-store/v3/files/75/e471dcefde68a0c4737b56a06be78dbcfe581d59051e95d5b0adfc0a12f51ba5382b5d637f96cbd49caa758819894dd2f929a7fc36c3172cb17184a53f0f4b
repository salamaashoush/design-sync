import { OverrideComponentProps } from "@kobalte/utils";
import { AsChildProp } from "../polymorphic";
export interface CheckboxIndicatorOptions extends AsChildProp {
    /**
     * Used to force mounting when more control is needed.
     * Useful when controlling animation with SolidJS animation libraries.
     */
    forceMount?: boolean;
}
export interface CheckboxIndicatorProps extends OverrideComponentProps<"div", CheckboxIndicatorOptions> {
}
/**
 * The visual indicator rendered when the checkbox is in a checked or indeterminate state.
 * You can style this element directly, or you can use it as a wrapper to put an icon into, or both.
 */
export declare function CheckboxIndicator(props: CheckboxIndicatorProps): import("solid-js").JSX.Element;
