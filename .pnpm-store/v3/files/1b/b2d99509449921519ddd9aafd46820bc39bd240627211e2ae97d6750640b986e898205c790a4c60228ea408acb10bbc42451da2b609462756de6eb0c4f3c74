import { OverrideComponentProps } from "@kobalte/utils";
import { AsChildProp } from "../polymorphic";
export interface RadioGroupItemDescriptionProps extends OverrideComponentProps<"div", AsChildProp> {
}
/**
 * The description that gives the user more information on the radio button.
 */
export declare function RadioGroupItemDescription(props: RadioGroupItemDescriptionProps): import("solid-js").JSX.Element;
