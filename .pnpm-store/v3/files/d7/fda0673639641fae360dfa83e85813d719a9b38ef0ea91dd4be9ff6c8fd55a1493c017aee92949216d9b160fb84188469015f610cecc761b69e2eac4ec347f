import { OverrideComponentProps } from "@kobalte/utils";
import { JSX } from "solid-js";
import { AsChildProp } from "../polymorphic";
import { CollectionNode } from "../primitives";
export interface ListboxItemOptions extends AsChildProp {
    /** The collection node to render. */
    item: CollectionNode;
}
export interface ListboxItemProps extends OverrideComponentProps<"li", ListboxItemOptions> {
}
/**
 * An item of the listbox.
 */
export declare function ListboxItem(props: ListboxItemProps): JSX.Element;
