import { OverrideComponentProps } from "@kobalte/utils";
import { JSX } from "solid-js";
import { AsChildProp } from "../polymorphic";
export interface HoverCardContentOptions extends AsChildProp {
    /** The HTML styles attribute (object form only). */
    style?: JSX.CSSProperties;
}
export interface HoverCardContentProps extends OverrideComponentProps<"div", HoverCardContentOptions> {
}
/**
 * Contains the content to be rendered when the hovercard is open.
 */
export declare function HoverCardContent(props: HoverCardContentProps): JSX.Element;
