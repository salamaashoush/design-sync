import { h } from 'preact';
import { Container } from '../container.js';
export default {
    parameters: {
        fixedWidth: true
    },
    title: 'Layout/Container'
};
export const ExtraSmallSpace = function () {
    const style = { backgroundColor: 'var(--figma-color-bg-brand-tertiary)' };
    return (h(Container, { space: "extraSmall", style: style }, "Text"));
};
export const SmallSpace = function () {
    const style = { backgroundColor: 'var(--figma-color-bg-brand-tertiary)' };
    return (h(Container, { space: "small", style: style }, "Text"));
};
export const MediumSpace = function () {
    const style = { backgroundColor: 'var(--figma-color-bg-brand-tertiary)' };
    return (h(Container, { space: "medium", style: style }, "Text"));
};
export const LargeSpace = function () {
    const style = { backgroundColor: 'var(--figma-color-bg-brand-tertiary)' };
    return (h(Container, { space: "large", style: style }, "Text"));
};
export const ExtraLargeSpace = function () {
    const style = { backgroundColor: 'var(--figma-color-bg-brand-tertiary)' };
    return (h(Container, { space: "extraLarge", style: style }, "Text"));
};
