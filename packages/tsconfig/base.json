{
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmit": true,
    "isolatedModules": true,
    "allowImportingTsExtensions": true
  }
}
