{
  "name": "@design-sync/storage",
  "version": "0.1.0",
  "description": "File storage utils and clients",
  "repository": "salamaashoush/design-sync",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "private": true,
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "pnpm run lint --fix",
    "test:types": "tsc --noEmit --skipLibCheck",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@design-sync/utils": "workspace:*"
  },
  "devDependencies": {
    "@design-sync/eslint-config": "workspace:*",
    "@design-sync/tsconfig": "workspace:*",
    "@figma/plugin-typings": "^1.88.0",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  }
}
