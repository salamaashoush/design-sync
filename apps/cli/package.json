{
  "name": "@design-sync/sync",
  "description": "CLI for Design Sync",
  "version": "0.0.0",
  "repository": "salamaashoush/tokenize",
  "license": "MIT",
  "sideEffects": false,
  "private": false,
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "unbuild",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && pnpm publish",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@design-sync/utils": "workspace:*",
    "@design-sync/w3c-dtfm": "workspace:*",
    "citty": "^0.1.4",
    "consola": "^3.2.3",
    "giget": "^1.1.3",
    "c12": "^1.5.1"
  },
  "devDependencies": {
    "@design-sync/eslint-config": "workspace:*",
    "@design-sync/tsconfig": "workspace:*",
    "@vanilla-extract/css": "^1.14.0",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "tsx": "^4.1.1",
    "@types/node": "^20.9.0",
    "@vitest/coverage-v8": "^0.34.6",
    "changelogen": "^0.5.5",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "bin": {
    "design-sync": "bin/design-sync.mjs"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  }
}
