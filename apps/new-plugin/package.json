{
  "name": "@tokenize/figma-plugin-2",
  "version": "0.0.0",
  "description": "Tokenize Figma Plugin",
  "dependencies": {
    "@create-figma-plugin/ui": "^2.6.1",
    "@create-figma-plugin/utilities": "^2.6.1",
    "@modular-forms/preact": "^0.6.2",
    "@preact/signals": "^1.2.1",
    "@tokenize/storage": "workspace:*",
    "@tokenize/utils": "workspace:*",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/recipes": "^0.5.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "clsx": "^2.0.0",
    "preact": "^10.17.1",
    "tinycolor2": "^1.6.0",
    "valibot": "^0.17.0",
    "@tanstack/react-query": "5.0.0-beta.31",
    "@dessert-box/react": "^0.7.5"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.6.1",
    "@create-figma-plugin/tsconfig": "^2.6.1",
    "@figma/plugin-typings": "1.77.0",
    "@hyrious/esbuild-plugin-style": "0.3.5",
    "@tokenize/design-tokens": "workspace:*",
    "@tokenize/rpc": "workspace:*",
    "@tokenize/tsconfig": "workspace:*",
    "@tokenize/utils": "workspace:*",
    "@tokenize/w3c-tokens": "workspace:*",
    "@types/tinycolor2": "^1.4.4",
    "@vanilla-extract/esbuild-plugin": "^2.3.0",
    "@vanilla-extract/integration": "^6.2.2",
    "esbuild": "^0.19.3",
    "typescript": "^5.2.2",
    "lint-staged": "^14.0.1",
    "eslint": "8.49.0"
  },
  "scripts": {
    "verify": "tsc --pretty",
    "build": "build-figma-plugin --minify",
    "watch": "build-figma-plugin --watch"
  },
  "figma-plugin": {
    "editorType": [
      "dev",
      "figma"
    ],
    "capabilities": [
      "inspect"
    ],
    "permissions": [
      "teamlibrary"
    ],
    "id": "tokenize-2",
    "name": "Tokenize 2",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
